{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "parser": "babel-eslint"
    },
    "rules": {
        "no-console":"off", 
        "linebreak-style": ["error", "windows"], 
        "no-underscore-dangle":"off", 
        "camelcase":"off",
        "indent": ["error", 2, 4, 6, 8]
    }
}
